        Section:NewButton("The Ruins Easy", "Creates Dungeon: The Ruins Easy", function()
            local A_1 = game:GetService("ReplicatedStorage").Dungeons["The Ruins Easy"]
            local A_2 = "A player made dungeon!"
            local Event = game:GetService("ReplicatedStorage").SendServer.CreateDungeon
            Event:FireServer(A_1, A_2)
        end)
    
    
