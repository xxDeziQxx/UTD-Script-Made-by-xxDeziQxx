if game.PlaceId == 4826840032 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Undertale Dungeons Script Hub", "Sentinel")


    -- Infinite Yield. Use commands like : Fly, remote control and others. NOTE: GodMode doesn't work, it will kill You
    local Tab = Window:NewTab("Main")
    local Section = Tab:NewSection("Main Scripts")
    Section:NewButton("Infinite Yield", "Attaching Infinite Yield", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end)
    
     -- Simply sets Your WalkSpeed to 70 and JumpPower to 100. Die to reset effect
    Section:NewButton("Im'a fast boi", "Perm sets Your Speed to 70 and Jumppower to 100", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/xxDeziQxx/Speedy/main/Speedy%20and%20Jumpy"))()
    end)


    -- NOTE: You cannot Disable it, only rejoining will disable
    Section:NewButton("Devestating Slash Permament", "Keep using Devestating Slash every 2 seconds", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/xxDeziQxx/Script/main/source'))()
    end)

    -- Close/Open UI
    Section:NewKeybind("Close/Open UI", "Choose Keybind to close/open UI. DEFAULT = TAB", Enum.KeyCode.Tab, function()
        Library:ToggleUI()
    end)


    -- Spells NOTE: You can change spells simply just by replacing Names of spells to spells name you want to use. You need to change name in keybinds too.
    local Tab = Window:NewTab("Spells")
    local Section = Tab:NewSection("Spells")
    Section:NewButton("Devilsknife", "Using Devilsknife", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Devilsknife") 
    end)
    Section:NewButton("Devestating Slash", "Using Devestating Slash ", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Devestating Slash") 
    end)
    Section:NewButton("Toxins", "Using Toxins ", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Toxins")
    end)
    Section:NewButton("Mana Lash", "Using Mana Lash", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Mana Lash")
    end)
    Section:NewButton("Deal Heal", "Using Deal Heal", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Deal Heal")
    end)
    Section:NewButton("Chaos Heal", "Using Chaos Heal", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Chaos Heal")
    end)
    Section:NewButton("Endless Stabs", "Using Endless Stabs", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Endless Stabs")
    end)
    Section:NewButton("Riptide", "Using Riptide", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Riptide")
    end)
    Section:NewButton("Supreme Damage Presents", "Using Supreme Damage Presents", function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Supreme Damage Presents")
    end)


    -- Keybinds (You can Change Them)
    local Section = Tab:NewSection("Keybinds")
    Section:NewKeybind("Devilsknife", "Press E", Enum.KeyCode.E, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Devilsknife")
    end)
    Section:NewKeybind("Devestating Slash", "Press Q", Enum.KeyCode.Q, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Devestating Slash")
    end)
    Section:NewKeybind("Toxins", "Press R", Enum.KeyCode.R, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Toxins")
    end)
    Section:NewKeybind("Mana Lash", "Press T", Enum.KeyCode.T, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Mana Lash")
    end)
    Section:NewKeybind("Deal Heal", "Press F", Enum.KeyCode.F, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Deal Heal")
    end)
    Section:NewKeybind("Chaos Heal", "Press Z", Enum.KeyCode.Z, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Chaos Heal")
    end)
    Section:NewKeybind("Endless Stabs", "Press X", Enum.KeyCode.X, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Endless Stabs")
    end)
    Section:NewKeybind("Riptide", "Press C", Enum.KeyCode.C, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Riptide")
    end)
    Section:NewKeybind("Supreme Damage Presents", "Press V", Enum.KeyCode.V, function()
        game:service'ReplicatedStorage'.UseSpell:FireServer("Supreme Damage Presents")
    end)
end

if game.PlaceId == 2848920787 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Undertale Boss Battles", "Sentinel")


    local Tab = Window:NewTab("Main")
    local Section = Tab:NewSection("Main Scripts")
    Section:NewButton("Infinite Yield", "Attaching Infinite Yield", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end)


    Section:NewButton("Im'a fast boi", "Perm sets Your Speed to 70 (Die to reset)", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/xxDeziQxx/Speedy/main/Speedy%20and%20Jumpy"))()
    end)

end
